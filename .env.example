# ReflexRuntime Environment Configuration
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Specify OpenAI model (defaults to gpt-4)
# OPENAI_MODEL=gpt-4

# Optional: Enable debug mode (defaults to false)
# DEBUG=true

# Optional: Custom debug log directory (defaults to ./debug)
# DEBUG_LOG_DIR=./debug
